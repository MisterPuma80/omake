<?xml version="1.0" encoding="UTF-8" ?>
<class name="Omake" inherits="Object" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../doc/class.xsd">
	<brief_description>
		A singleton for holding the Omake functions.
	</brief_description>
	<description>
		Some things that do stuff.
	</description>
	<tutorials>
	</tutorials>
	<constants>
		<constant name="INT64_MAX" value="9223372036854775807">
			Max signed 64 bit int value.
		</constant>
		<constant name="INT64_MIN" value="-9223372036854775808">
			Min signed 64 bit int value.
		</constant>
	</constants>
	<methods>
		<method name="test_func" qualifiers="static">
			<return type="int" />
			<description>
				Returns 69.
			</description>
		</method>
		<method name="add_clampedi" qualifiers="static">
			<return type="int" />
			<param index="0" name="a" type="int" />
			<param index="1" name="b" type="int" />
			<param index="2" name="min" type="int" default="-9223372036854775808" />
			<param index="3" name="max" type="int" default="9223372036854775807" />
			<description>
				Returns a + b, but will return max instead of overflow, or min instead of underflow.
				By default min is INT64_MIN and max is INT64_MAX.
			</description>
		</method>
		<method name="get_cpu_ticks_nsec" qualifiers="static">
			<return type="int" />
			<description>
				Returns the CPU ticks in nanoseconds.
			</description>
		</method>
	</methods>
</class>
